{
  "name": "damx-bttc-stats",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "create-local": "graph create damx-io/damx-bttc-stats --node http://127.0.0.1:8020",
    "deploy-local": "graph deploy damx-io/damx-bttc-stats --debug --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "remove-local": "graph remove --node http://localhost:8020/ damx-io/damx-bttc-stats",
    "deploy-prod": "graph deploy --product hosted-service damx-io/damx-bttc-stats",
    "deploy-test": "graph deploy --product hosted-service zraccoon/damx-bttc-stats-staging --deploy-key $(cat .access_token)"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.21.1",
    "@graphprotocol/graph-ts": "0.20.0",
    "ethers": "^5.4.6",
    "moment": "^2.29.1"
  }
}
